{
    "protocol" : {
        "connection":{
            "acceptorThreads":"1",
            "clientToProxyWorkerThreads":"40",
            "proxyToServerWorkerThreads":"40",
            "timeout":"40000",
            "idleTimeout":"60"
        },
        "tls":{
            "enable":"false",
            "certificateFile":"",
            "privateKeyFile":"",
            "privateKeyPassword":""
        },
        "http11":{
        },
        "http20":{
            "enable":"false",
            "maxContentLength":"65536000"
        },
        "websocket":{
            "requestMaxNumber":"2000",
            "maxFramePayloadLength":"65536000",
            "idleTimeout":"600"
        }
    },
    "html": {
        "support": [
            {
                "fileType":"woff",
                "contentType":"application/font-woff"
            },
            {
                "fileType": "tff",
                "contentType":"application/font-ttf"
            },
            {
                "fileType": "map",
                "contentType":"application/font-map"
            }
        ]
    },
    "cors" : {
        "enable":"false",
        "allowCredentials":"true",
        "maxAge":"600",
        "allowOrigin": [
            {"value":"*"}
        ],
        "allowHeaders": [
            {"value":"*"}
        ],
        "allowMethods": [
            {"value":"*"}
        ],
        "exposeHeaders": [
            {"value":"*"}
        ]
    },
    "proxy": [
        {
          "127.0.0.1;localhost": {
            "/": {
                "index": "index.html",
                "root": "c:/projects/banyan-admin-fe",
                "proxy": "127.0.0.1:9140"
            },
            "/gateway": {
                "proxy": "127.0.0.1:9140"
            },
            "/banyanSystem": {
                "proxy": "127.0.0.1:9140"
            },
            "/BanyanAdmin": {
                "proxy": "127.0.0.1:8787"
            },
            "/banyanNDI": {
                "proxy": "127.0.0.1:60008"
            },
            "/FileBrowser/api": {
                "proxy": "192.168.1.28",
                "rewrite":"/api"
            },
            "/FileBrowser": {
                "index": "index.html",
                "root": "c:/projects/banyan-admin-filebrowser-fe",
                "rewrite":"/",
                "404":"index.html",
                "error":"index.html"
            }
          }
        }
    ]
}
